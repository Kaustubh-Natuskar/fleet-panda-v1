{
  "name": "fleet-tracking-api",
  "version": "1.0.0",
  "description": "Real-time fleet, driver, and delivery tracking platform for FuelPanda",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
