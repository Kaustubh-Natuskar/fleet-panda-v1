    "joi": "17.13.3",
    "marked": "^17.0.1",
    "morgan": "1.10.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1"
  },
  "devDependencies": {
    "eslint": "9.17.0",
    "jest": "29.7.0",
    "nodemon": "3.1.9",
    "prisma": "6.19.2",
    "supertest": "7.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
